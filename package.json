{
  "name": "@bicou/directus-extension-tiptap",
  "description": "Tiptap rich text editor interface and display for directus",
  "icon": "extension",
  "version": "1.18.5",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-bundle",
    "directus-custom-interface",
    "directus-custom-display",
    "tiptap",
    "prosemirror",
    "wysiwg",
    "editor"
  ],
  "homepage": "https://github.com/gbicou/directus-extension-tiptap",
  "author": "Benjamin VIELLARD <bicou@bicou.com>",
  "license": "MIT",
  "repository": "github:gbicou/directus-extension-tiptap",
  "type": "module",
  "directus:extension": {
    "host": "^11.0.0",
    "type": "bundle",
    "path": {
      "app": "dist/app.mjs",
      "api": "dist/api.mjs"
    },
    "entries": [
      {
        "type": "interface",
        "name": "directus-extension-tiptap-interface",
        "source": "src/interface.ts"
      },
      {
        "type": "display",
        "name": "directus-extension-tiptap-display",
        "source": "src/display.ts"
      }
    ]
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "dependencies": {
    "@tiptap/core": "2.11.5",
    "@tiptap/extension-character-count": "2.11.5",
    "@tiptap/extension-focus": "2.11.5",
    "@tiptap/extension-highlight": "2.11.5",
    "@tiptap/extension-link": "2.11.5",
    "@tiptap/extension-placeholder": "2.11.5",
    "@tiptap/extension-subscript": "2.11.5",
    "@tiptap/extension-superscript": "2.11.5",
    "@tiptap/extension-table": "2.11.5",
    "@tiptap/extension-table-cell": "2.11.5",
    "@tiptap/extension-table-header": "2.11.5",
    "@tiptap/extension-table-row": "2.11.5",
    "@tiptap/extension-task-item": "2.11.5",
    "@tiptap/extension-task-list": "2.11.5",
    "@tiptap/extension-text-align": "2.11.5",
    "@tiptap/extension-typography": "2.11.5",
    "@tiptap/extension-underline": "2.11.5",
    "@tiptap/pm": "2.11.5",
    "@tiptap/starter-kit": "2.11.5",
    "@tiptap/suggestion": "2.11.5"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "13.0.4",
    "@directus/schema": "13.0.1",
    "@directus/sdk": "19.1.0",
    "@directus/types": "13.1.0",
    "@eslint/js": "9.23.0",
    "@tiptap/vue-3": "2.11.5",
    "@vue/tsconfig": "0.7.0",
    "dotenv": "16.4.7",
    "eslint": "9.23.0",
    "eslint-plugin-prettier": "5.2.5",
    "eslint-plugin-vue": "10.0.0",
    "prettier": "3.5.3",
    "rollup": "4.37.0",
    "rollup-plugin-node-externals": "8.0.0",
    "sass": "1.86.0",
    "tippy.js": "6.3.7",
    "typescript": "5.8.2",
    "typescript-eslint": "8.28.0",
    "vitest": "3.0.9",
    "vue": "3.5.13",
    "vue-i18n": "10.0.6"
  },
  "peerDependencies": {
    "@directus/extensions-sdk": "13.0.4",
    "vue": "3.5.13",
    "vue-i18n": "10.0.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.6.5+sha512.cdf928fca20832cd59ec53826492b7dc25dc524d4370b6b4adbf65803d32efaa6c1c88147c0ae4e8d579a6c9eec715757b50d4fa35eea179d868eada4ed043af",
  "optionalDependencies": {
    "@tiptap-pro/extension-emoji": "2.17.5",
    "@tiptap-pro/extension-invisible-characters": "2.17.5",
    "@tiptap-pro/extension-mathematics": "2.17.5",
    "@tiptap-pro/extension-unique-id": "2.17.5",
    "katex": "0.16.21"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "vue-demi"
    ]
  }
}
