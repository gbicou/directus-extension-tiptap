{
  "name": "@bicou/directus-extension-tiptap",
  "description": "Tiptap rich text editor interface and display for directus",
  "icon": "extension",
  "version": "1.18.3",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-bundle",
    "directus-custom-interface",
    "directus-custom-display",
    "tiptap",
    "prosemirror",
    "wysiwg",
    "editor"
  ],
  "homepage": "https://github.com/gbicou/directus-extension-tiptap",
  "author": "Benjamin VIELLARD <bicou@bicou.com>",
  "license": "MIT",
  "repository": "github:gbicou/directus-extension-tiptap",
  "type": "module",
  "directus:extension": {
    "host": "^11.0.0",
    "type": "bundle",
    "path": {
      "app": "dist/app.mjs",
      "api": "dist/api.mjs"
    },
    "entries": [
      {
        "type": "interface",
        "name": "directus-extension-tiptap-interface",
        "source": "src/interface.ts"
      },
      {
        "type": "display",
        "name": "directus-extension-tiptap-display",
        "source": "src/display.ts"
      }
    ]
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "lint": "eslint .",
    "release": "changeset publish",
    "test": "vitest run"
  },
  "dependencies": {
    "@tiptap/core": "2.11.3",
    "@tiptap/extension-character-count": "2.11.3",
    "@tiptap/extension-focus": "2.11.3",
    "@tiptap/extension-highlight": "2.11.3",
    "@tiptap/extension-link": "2.11.3",
    "@tiptap/extension-placeholder": "2.11.3",
    "@tiptap/extension-subscript": "2.11.3",
    "@tiptap/extension-superscript": "2.11.3",
    "@tiptap/extension-table": "2.11.3",
    "@tiptap/extension-table-cell": "2.11.3",
    "@tiptap/extension-table-header": "2.11.3",
    "@tiptap/extension-table-row": "2.11.3",
    "@tiptap/extension-task-item": "2.11.3",
    "@tiptap/extension-task-list": "2.11.3",
    "@tiptap/extension-text-align": "2.11.3",
    "@tiptap/extension-typography": "2.11.3",
    "@tiptap/extension-underline": "2.11.3",
    "@tiptap/pm": "2.11.3",
    "@tiptap/starter-kit": "2.11.3",
    "@tiptap/suggestion": "2.11.3"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.12",
    "@directus/extensions-sdk": "12.1.4",
    "@directus/schema": "12.1.1",
    "@directus/sdk": "18.0.3",
    "@directus/types": "12.2.2",
    "@eslint/js": "9.19.0",
    "@tiptap/vue-3": "2.11.3",
    "@vue/tsconfig": "0.7.0",
    "dotenv": "16.4.7",
    "eslint": "9.19.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-vue": "9.32.0",
    "prettier": "3.4.2",
    "rollup": "4.32.0",
    "rollup-plugin-node-externals": "8.0.0",
    "sass": "1.83.4",
    "tippy.js": "6.3.7",
    "typescript": "5.4.5",
    "typescript-eslint": "8.21.0",
    "vitest": "3.0.4",
    "vue": "3.5.13",
    "vue-i18n": "10.0.5",
    "wait-on": "8.0.2"
  },
  "peerDependencies": {
    "@directus/extensions-sdk": "12.1.4",
    "vue": "3.5.13",
    "vue-i18n": "10.0.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0",
  "optionalDependencies": {
    "@tiptap-pro/extension-emoji": "2.17.2",
    "@tiptap-pro/extension-invisible-characters": "2.17.2",
    "@tiptap-pro/extension-mathematics": "2.17.2",
    "@tiptap-pro/extension-unique-id": "2.17.2",
    "katex": "0.16.21"
  }
}
