{
  "name": "@bicou/directus-extension-tiptap",
  "description": "Tiptap rich text editor interface and display for directus",
  "icon": "extension",
  "version": "1.10.2",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-bundle",
    "directus-custom-interface",
    "directus-custom-display",
    "tiptap",
    "prosemirror",
    "wysiwg",
    "editor"
  ],
  "homepage": "https://github.com/gbicou/directus-extension-tiptap",
  "author": "Benjamin VIELLARD <bicou@bicou.com>",
  "license": "MIT",
  "repository": "github:gbicou/directus-extension-tiptap",
  "type": "module",
  "directus:extension": {
    "host": "^10.1.0",
    "type": "bundle",
    "path": {
      "app": "dist/app.mjs",
      "api": "dist/api.mjs"
    },
    "entries": [
      {
        "type": "interface",
        "name": "directus-extension-tiptap-interface",
        "source": "src/interface.ts"
      },
      {
        "type": "display",
        "name": "directus-extension-tiptap-display",
        "source": "src/display.ts"
      }
    ]
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "lint": "eslint .",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@bicou/semantic-release": "^1.0.1",
    "@directus/extensions-sdk": "^10.1.4",
    "@directus/schema": "^10.0.1",
    "@directus/types": "^10.1.1",
    "@rushstack/eslint-patch": "^1.3.2",
    "@tiptap/core": "^2.0.3",
    "@tiptap/extension-character-count": "^2.0.3",
    "@tiptap/extension-focus": "^2.0.3",
    "@tiptap/extension-highlight": "^2.0.3",
    "@tiptap/extension-link": "^2.0.3",
    "@tiptap/extension-placeholder": "^2.0.3",
    "@tiptap/extension-subscript": "^2.0.3",
    "@tiptap/extension-superscript": "^2.0.3",
    "@tiptap/extension-table": "^2.0.3",
    "@tiptap/extension-table-cell": "^2.0.3",
    "@tiptap/extension-table-header": "^2.0.3",
    "@tiptap/extension-table-row": "^2.0.3",
    "@tiptap/extension-task-item": "^2.0.3",
    "@tiptap/extension-task-list": "^2.0.3",
    "@tiptap/extension-text-align": "^2.0.3",
    "@tiptap/extension-typography": "^2.0.3",
    "@tiptap/extension-underline": "^2.0.3",
    "@tiptap/pm": "^2.0.3",
    "@tiptap/starter-kit": "^2.0.3",
    "@tiptap/vue-3": "^2.0.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.43.0",
    "eslint-plugin-vue": "^9.15.0",
    "prettier": "^2.8.8",
    "rollup": "^3.25.1",
    "rollup-plugin-node-externals": "^6.1.1",
    "sass": "^1.63.4",
    "semantic-release": "^21.0.5",
    "typescript": "~5.1.0",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2"
  },
  "peerDependencies": {
    "@directus/extensions-sdk": "^10.1.3",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@8.6.2"
}
